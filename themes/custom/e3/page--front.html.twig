
<div class="layout-container">

  <header role="banner">
    {{ page.header }}
  </header>

<div role="jumbotron">
  {{ page.jumbotron }}
</div>

  <main role="main">
    <a id="main-content" tabindex="-1"></a>{# link is in html.html.twig #}

    <div class="layout-content">
      {{ page.content_title }}
    </div>{# /.layout-content #}

    <div class="layout-content">
      {{ page.content }}
    </div>{# /.layout-content #}
  </main>

</div>{# /.layout-container #}
